{{docs-header logo='ember-cli' addonLogo='ember-cli'}}

{{#docs-viewer as |viewer|}}
  {{#viewer.nav as |nav|}}
    {{nav.item 'Overview' 'docs.index'}}
    {{nav.item 'Installation' 'docs.installation'}}

    {{nav.section 'Features'}}
    {{nav.item 'Yield to Children' 'docs.features.children'}}
  {{/viewer.nav}}

  {{#viewer.main}}
    <div class="docs-container docs__center">
      <div class="docs-section">
        {{outlet}}
      </div>
    </div>
  {{/viewer.main}}
{{/docs-viewer}}
